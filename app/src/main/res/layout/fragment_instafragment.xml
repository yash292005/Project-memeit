<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Instafragment">
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/Fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end|bottom"
        android:layout_marginBottom="65dp"
        android:layout_marginEnd="20dp"
        android:contentDescription="@string/this_does_something"
        android:src="@drawable/ic_baseline_search_24"
        app:backgroundTint="?attr/color"
        tools:ignore="SpeakableTextPresentCheck" />
   <androidx.recyclerview.widget.RecyclerView
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       tools:listitem="@layout/wall_paper_item"
       android:id="@+id/WallpaperView"/>

   <ProgressBar
       android:layout_width="100dp"
       android:layout_height="100dp"
       android:layout_gravity="center"
       android:id="@+id/wallpaperLoader"
       />

</FrameLayout>